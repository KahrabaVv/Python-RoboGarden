import math
# Take the required input from the user
num = int(input("Enter a number to print Prime Numbers: "))
# Initialize the list to store the prime numbers
prime_numbers = []
for i in range (2, num+1):
      for j in range(2, int(math.sqrt(i))+1):
          if i%j == 0:
              break
      else:
              prime_numbers.append(i)
print(prime_numbers)              
